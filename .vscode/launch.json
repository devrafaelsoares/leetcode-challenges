{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/**/*.ts",
            "runtimeArgs": ["node -r", "ts-node/register", "-r", "tsconfig-paths/register"],
            "outFiles": ["${workspaceFolder}/**/*.js"]
        }
    ]
}
